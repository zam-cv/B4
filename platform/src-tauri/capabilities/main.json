{
  "$schema": "./schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "window:allow-start-dragging",
    "app:default",
    "websocket:default",
    "websocket:allow-connect",
    "os:allow-platform",
    "resources:default",
    "menu:default",
    "tray:default",
    "shell:allow-open"
  ]
}
